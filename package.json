{
  "name": "api",
  "version": "0.0.1",
  "description": "api-for-order-together",
  "author": "piotr.solarz-wnek",
  "license": "",
  "scripts": {
    "format": "prettier --write \"**/*.ts\"",
    "format:watch": "onchange \"**/*.ts\" -- prettier --write {{changed}}",
    "webpack:dev": "webpack --config webpack.config.dev.js",
    "webpack:prod": "webpack --config webpack.config.prod.js",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "nodemon",
    "clean:dist": "rm -rf dist",
    "run:prod": "node dist/bundle.js",
    "start:prod": "npm-run-all clean:dist webpack:prod run:prod",
    "start:hmr": "node dist/server",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^5.0.0",
    "@nestjs/core": "^5.0.0",
    "@nestjs/microservices": "^5.0.0",
    "@nestjs/testing": "^5.0.0",
    "@nestjs/websockets": "^5.0.0",
    "dotenv": "^6.0.0",
    "fastify-formbody": "^2.0.0",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.0.0",
    "typescript": "^2.6.2"
  },
  "devDependencies": {
    "@types/dotenv": "^4.0.3",
    "@types/express": "^4.0.39",
    "@types/jest": "^21.1.8",
    "@types/node": "^9.3.0",
    "@types/supertest": "^2.0.4",
    "jest": "^21.2.1",
    "nodemon": "^1.14.1",
    "npm-run-all": "^4.1.3",
    "onchange": "^4.0.0",
    "prettier": "^1.11.1",
    "supertest": "^3.0.0",
    "ts-jest": "^21.2.4",
    "ts-loader": "^4.1.0",
    "ts-node": "^4.1.0",
    "tsconfig-paths": "^3.1.1",
    "tsconfig-paths-webpack-plugin": "^3.1.4",
    "tslint": "5.3.2",
    "tslint-config-prettier": "^1.13.0",
    "webpack": "^4.2.0",
    "webpack-cli": "^2.0.13",
    "webpack-node-externals": "^1.6.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage"
  }
}
